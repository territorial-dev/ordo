{
  "recipe": [
    {
      "id": "reproject",
      "type": "REPROJECT_LAS",
      "params": {
        "source_epsg": "X",
        "target_epsg": "Y"
      },
      "inputs": {
        "input_las": "input_las"
      },
      "outputs": ["output_las"]
    },
    {
      "id": "dem",
      "type": "GENERATE_DEM",
      "params": {
        "resolution": 1.0,
        "method": "tin"
      },
      "inputs": {
        "input_las": "output_las"
      },
      "outputs": ["output_dem"]
    },
    {
      "id": "hillshade",
      "type": "GENERATE_HILLSHADE",
      "params": {
        "azimuth": 315,
        "altitude": 45
      },
      "inputs": {
        "input_dem": "output_dem"
      },
      "outputs": ["output_hillshade"]
    },
    {
      "id": "contours",
      "type": "GENERATE_CONTOURS",
      "params": {
        "interval": 1.0
      },
      "inputs": {
        "input_dem": "output_dem"
      },
      "outputs": ["output_contours"]
    },
    {
      "id": "ept",
      "type": "BUILD_EPT",
      "params": {
        "resolution": "auto"
      },
      "inputs": {
        "input_las": "output_las"
      },
      "outputs": ["output_ept"]
    }
  ]
}
